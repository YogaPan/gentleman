{
  "name": "Gentleman",
  "version": "0.0.1",
  "description": "Gentleman",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 3,
  "permissions": ["tabs", "contextMenus"],
  "omnibox": {
    "keyword": "GEN"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Gentleman",
    "default_popup": "popup.html"
  },
  "commands": {
    "push": {
      "suggested_key": {
        "windows": "Ctrl+Shift+K",
        "mac": "Command+Shift+K",
        "chromeos": "Ctrl+Shift+K",
        "linux": "Ctrl+Shift+K"
      },
      "description": "push all NSFW tabs"
    },
    "pop": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z",
        "chromeos": "Ctrl+Shift+Z",
        "linux": "Ctrl+Shift+Z"
      },
      "description": "pop all NSFW tabs"
    }
  }
}
